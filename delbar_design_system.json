{
  "brandIdentity": {
    "primaryColors": {
      "warmTerracotta": "#E67E22",
      "coralPink": "#F8B5B5",
      "sageMist": "#7D9B8A",
      "charcoalGray": "#2C3E50",
      "creamWhite": "#FDF6F0"
    },
    "secondaryColors": {
      "deepRed": "#C0392B",
      "mutedTeal": "#5D8A8A",
      "lightBlush": "#F4E4E4",
      "warmGray": "#95A5A6",
      "pureWhite": "#FFFFFF"
    }
  },
  "colorPalette": {
    "backgrounds": {
      "primary": "#FDF6F0",
      "secondary": "#F8B5B5",
      "accent": "#E67E22",
      "neutral": "#FFFFFF",
      "overlay": "rgba(244, 228, 228, 0.9)"
    },
    "gradients": {
      "heroSection": "linear-gradient(135deg, #F8B5B5 0%, #E67E22 100%)",
      "cardOverlay": "linear-gradient(180deg, rgba(0,0,0,0) 0%, rgba(0,0,0,0.3) 100%)",
      "buttonHover": "linear-gradient(135deg, #D35400 0%, #C0392B 100%)"
    },
    "text": {
      "primary": "#2C3E50",
      "secondary": "#5D6D7E",
      "accent": "#E67E22",
      "inverse": "#FFFFFF",
      "muted": "#95A5A6"
    },
    "borders": {
      "light": "#E8E8E8",
      "medium": "#D5D5D5",
      "accent": "#E67E22",
      "focus": "#3498DB"
    }
  },
  "elementStyling": {
    "navigationHeader": {
      "background": "#FFFFFF",
      "borderBottom": "1px solid #E8E8E8",
      "shadow": "0 2px 8px rgba(0,0,0,0.08)",
      "logo": {
        "textColor": "#2C3E50",
        "accentColor": "#E67E22"
      },
      "menuItems": {
        "textColor": "#2C3E50",
        "fontSize": "14px",
        "fontWeight": "500",
        "letterSpacing": "0.5px",
        "hover": {
          "textColor": "#E67E22",
          "transition": "color 0.3s ease"
        }
      }
    },
    "heroSections": {
      "background": "linear-gradient(135deg, #F8B5B5 0%, #E67E22 100%)",
      "overlay": "rgba(244, 228, 228, 0.1)",
      "minHeight": "60vh",
      "textAlignment": "center",
      "padding": "80px 20px"
    },
    "cards": {
      "background": "#FFFFFF",
      "border": "none",
      "borderRadius": "12px",
      "shadow": "0 4px 20px rgba(0,0,0,0.08)",
      "padding": "24px",
      "hover": {
        "shadow": "0 8px 32px rgba(0,0,0,0.12)",
        "transform": "translateY(-4px)",
        "transition": "all 0.3s ease"
      },
      "imageContainer": {
        "borderRadius": "8px",
        "overflow": "hidden",
        "aspectRatio": "16:9"
      },
      "content": {
        "textColor": "#2C3E50",
        "spacing": "16px"
      }
    },
    "buttons": {
      "primary": {
        "background": "#E67E22",
        "textColor": "#FFFFFF",
        "borderRadius": "8px",
        "padding": "12px 24px",
        "fontSize": "14px",
        "fontWeight": "600",
        "letterSpacing": "0.5px",
        "border": "none",
        "hover": {
          "background": "#D35400",
          "transform": "translateY(-2px)",
          "shadow": "0 4px 12px rgba(230, 126, 34, 0.3)"
        }
      },
      "secondary": {
        "background": "transparent",
        "textColor": "#E67E22",
        "border": "2px solid #E67E22",
        "borderRadius": "8px",
        "padding": "10px 22px",
        "hover": {
          "background": "#E67E22",
          "textColor": "#FFFFFF"
        }
      },
      "ctaBooking": {
        "background": "#FF4757",
        "textColor": "#FFFFFF",
        "borderRadius": "6px",
        "padding": "10px 20px",
        "fontSize": "14px",
        "fontWeight": "600",
        "textTransform": "uppercase",
        "letterSpacing": "1px"
      }
    },
    "typography": {
      "headings": {
        "h1": {
          "fontSize": "3.5rem",
          "fontWeight": "300",
          "lineHeight": "1.2",
          "color": "#2C3E50",
          "marginBottom": "24px"
        },
        "h2": {
          "fontSize": "2.5rem",
          "fontWeight": "400",
          "lineHeight": "1.3",
          "color": "#2C3E50",
          "marginBottom": "20px"
        },
        "h3": {
          "fontSize": "1.8rem",
          "fontWeight": "500",
          "lineHeight": "1.4",
          "color": "#2C3E50",
          "marginBottom": "16px"
        }
      },
      "body": {
        "fontSize": "16px",
        "lineHeight": "1.6",
        "color": "#5D6D7E",
        "fontWeight": "400"
      },
      "caption": {
        "fontSize": "14px",
        "color": "#95A5A6",
        "fontWeight": "500",
        "textTransform": "uppercase",
        "letterSpacing": "0.5px"
      }
    },
    "sections": {
      "menuSection": {
        "background": "linear-gradient(135deg, #F8B5B5 0%, #E67E22 100%)",
        "padding": "60px 0",
        "tabNavigation": {
          "borderBottom": "2px solid rgba(255,255,255,0.3)",
          "activeTab": {
            "borderBottom": "2px solid #FFFFFF",
            "color": "#FFFFFF"
          },
          "inactiveTab": {
            "color": "rgba(255,255,255,0.8)"
          }
        }
      },
      "aboutSection": {
        "background": "#FDF6F0",
        "padding": "80px 0"
      },
      "locationSection": {
        "background": "#FFFFFF",
        "padding": "60px 0"
      }
    },
    "imageContainers": {
      "foodPhotos": {
        "borderRadius": "12px",
        "overflow": "hidden",
        "aspectRatio": "4:3",
        "shadow": "0 8px 24px rgba(0,0,0,0.12)"
      },
      "archFrames": {
        "borderRadius": "50% 50% 50% 50% / 60% 60% 40% 40%",
        "overflow": "hidden",
        "border": "4px solid #E67E22"
      }
    },
    "locationCards": {
      "background": "#FFFFFF",
      "borderRadius": "16px",
      "padding": "32px",
      "shadow": "0 6px 24px rgba(0,0,0,0.08)",
      "textAlign": "center",
      "hoursDisplay": {
        "color": "#5D6D7E",
        "fontSize": "14px",
        "lineHeight": "1.8"
      },
      "contactInfo": {
        "color": "#2C3E50",
        "fontSize": "16px",
        "fontWeight": "500"
      }
    },
    "socialIcons": {
      "size": "24px",
      "color": "#5D6D7E",
      "hover": {
        "color": "#E67E22",
        "transform": "scale(1.1)"
      }
    },
    "footer": {
      "background": "linear-gradient(135deg, #F8B5B5 0%, #E67E22 100%)",
      "textColor": "#2C3E50",
      "padding": "40px 0",
      "links": {
        "color": "#2C3E50",
        "hover": {
          "color": "#FFFFFF"
        }
      }
    }
  },
  "layoutSystem": {
    "container": {
      "maxWidth": "1200px",
      "margin": "0 auto",
      "padding": "0 20px"
    },
    "grid": {
      "columns": 12,
      "gutter": "24px",
      "breakpoints": {
        "mobile": "768px",
        "tablet": "1024px",
        "desktop": "1200px"
      }
    },
    "spacing": {
      "xs": "8px",
      "sm": "16px",
      "md": "24px",
      "lg": "32px",
      "xl": "48px",
      "xxl": "64px"
    }
  },
  "animations": {
    "hover": {
      "duration": "0.3s",
      "easing": "ease-out"
    },
    "cardHover": {
      "transform": "translateY(-4px)",
      "shadow": "0 8px 32px rgba(0,0,0,0.12)"
    },
    "buttonHover": {
      "transform": "translateY(-2px)",
      "shadow": "0 4px 12px rgba(230, 126, 34, 0.3)"
    }
  },
  "applicationRules": {
    "DO_APPLY": {
      "gradients": [
        "Hero section backgrounds",
        "Menu section backgrounds", 
        "Footer backgrounds",
        "Button hover states"
      ],
      "shadows": [
        "Card containers",
        "Image containers",
        "Navigation header",
        "Button hover states"
      ],
      "borderRadius": [
        "Cards (12px)",
        "Images (8-12px)",
        "Buttons (6-8px)",
        "Arch frames (custom)"
      ],
      "colorAccents": [
        "Primary buttons (#E67E22)",
        "Active navigation items",
        "CTA elements (#FF4757)",
        "Hover states"
      ]
    },
    "DO_NOT_APPLY": {
      "gradients": [
        "Text elements",
        "Icon fills",
        "Border colors",
        "Individual card content"
      ],
      "cardStyles": [
        "Do not apply card background gradients to icons",
        "Do not apply card shadows to text content",
        "Do not apply card border radius to inline elements"
      ],
      "buttonStyles": [
        "Do not apply button colors to regular text",
        "Do not apply button padding to icon containers",
        "Do not apply button hover effects to static elements"
      ],
      "textStyles": [
        "Do not apply background gradients to text",
        "Do not apply card shadows to typography",
        "Do not apply button styling to paragraph text"
      ]
    ]
  },
  "brandingElements": {
    "michelin": {
      "badgeColor": "#C0392B",
      "badgeText": "#FFFFFF",
      "position": "top-left",
      "borderRadius": "50%",
      "padding": "8px 12px"
    },
    "logoTreatment": {
      "primaryColor": "#2C3E50",
      "accentColor": "#E67E22",
      "underlineStyle": "decorative swash"
    },
    "culturalElements": {
      "archFrames": "Persian-inspired rounded rectangles",
      "colorPalette": "Warm, earthy Middle Eastern tones",
      "typography": "Elegant, readable sans-serif with script accents"
    }
  },
  "responsiveBreakpoints": {
    "mobile": {
      "maxWidth": "767px",
      "modifications": {
        "heroText": "fontSize: 2.5rem",
        "cardPadding": "16px",
        "buttonPadding": "10px 16px"
      }
    },
    "tablet": {
      "maxWidth": "1023px",
      "modifications": {
        "heroText": "fontSize: 3rem",
        "gridColumns": "2",
        "cardPadding": "20px"
      }
    },
    "desktop": {
      "minWidth": "1024px",
      "modifications": {
        "heroText": "fontSize: 3.5rem",
        "gridColumns": "3-4",
        "cardPadding": "24px"
      }
    }
  },
  "interactionStates": {
    "default": "Base styling as specified",
    "hover": "Enhanced shadows, slight transforms, color transitions",
    "active": "Pressed state with darker colors, inset shadows",
    "focus": "Visible focus rings using accent colors",
    "disabled": "Muted colors, reduced opacity, no interactions"
  }
}